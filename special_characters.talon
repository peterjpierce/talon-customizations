from talon import actions

air con a cento | span air:
    insert("á")

each con a cento | span each:
    insert("é")

sit con a cento | span sit:
    insert("í")

add con a cento | span odd:
    insert("ó")

urge con a cento | span urge:
    insert("ú")

darted urge:
    insert("ü")

span near:
    insert("ñ")

span question:
    insert("¿")

span bang:
    insert("¡")
